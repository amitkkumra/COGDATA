{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "amitadfs"
		},
		"AzureBlobStorageLS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorageLS'"
		},
		"OLAPDBLS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'OLAPDBLS'"
		},
		"OLTPLS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'OLTPLS'"
		},
		"BlobCreatedEvent_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/c30e25e7-8e0c-4019-b378-3c3abf2b6e91/resourceGroups/COGRG/providers/Microsoft.Storage/storageAccounts/amitacc"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "amitadfs_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorageLS_connectionString": {
						"value": "[parameters('AzureBlobStorageLS_connectionString')]"
					},
					"OLAPDBLS_connectionString": {
						"value": "[parameters('OLAPDBLS_connectionString')]"
					},
					"OLTPLS_connectionString": {
						"value": "[parameters('OLTPLS_connectionString')]"
					},
					"BlobCreatedEvent_properties_typeProperties_scope": {
						"value": "[parameters('BlobCreatedEvent_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}